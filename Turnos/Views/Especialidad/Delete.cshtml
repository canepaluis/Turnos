@model Turnos.Models.Especialidad

<h1>Eliminar</h1>
<h4>¿Está usted seguro de eliminar la siguiente especialidad?</h4>
<hr />
<div class="row">
    <div class="col s12 m6">
        <div class="card hoverable blue darken-1">
            <div class="card-content white-text">
                <span class="card-title"><h5><b>@Html.DisplayFor(modelo => modelo.Descripcion)</b></h5></span>
            </div>
            <div class="card-action blue-grey darken-2">
                <form asp-action="Delete">
                    <input type="hidden" asp-for="IdEspecialidad" />
                    <input type="submit" value="Eliminar" class="waves-effect waves-light btn-small orange lighten-2" />
                    <a asp-action="Index" class="right">Regresar</a>
                </form>
            </div>
        </div>
    </div>
</div>