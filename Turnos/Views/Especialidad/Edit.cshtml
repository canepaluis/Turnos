@model Turnos.Models.Especialidad

<h1>Editar</h1>

<h4>Especialidad</h4>
<hr />
<div class="row">
    <form class="col s12" asp-action="Edit">
        <div asp-validation-summary="ModelOnly"></div>
        <input type="hidden" asp-for="IdEspecialidad" />
        <div class="input-field col s12">
            <i class="material-icons prefix">medication</i>
            <label asp-for="Descripcion"></label>
            <input asp-for="Descripcion" />
            <span asp-validation-for="Descripcion"></span>
        </div>
        <div class="row">
            <div class="col s6 m6">
                <button class="btn waves-effect waves-light cyan accent-4" type="submit" name="action">
                    Guardar
                    <i class="material-icons right">send</i>
                </button>
            </div>
            <div class="col s6 m6">
                <a class="waves-effect waves-light btn grey darken-4" asp-action="Index"><i class="material-icons left">undo</i>Regresar</a>
            </div>
        </div>
    </form>
</div>